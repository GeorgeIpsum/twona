model Integration {
  id        String   @id @default(cuid())
  name      String   @unique
  type      String   @default("REALTIME") // ENUM THAT IS EITHER "REALTIME" OR "SUMMARY"
  url       String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  widgets Widget[]

  @@index(name)
  @@index(type)
}
