{
  "name": "twona",
  "namespace": "@twona",
  "packageManager": "pnpm@9.1.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "prepare": "husky",
    "clean": "rm -rf node_modules */node_modules **/node_modules **/.turbo .turbo **/.next **/dist **/pnpm-lock.yaml pnpm-lock.yaml && pnpm i",
    "commitlint": "commitlint --edit"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@tilli-pro/eslint-config": "^2.0.2",
    "@twona/typescript-config": "workspace:*",
    "commitlint": "^19.3.0",
    "eslint": "^9.4.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9"
      }
    },
    "patchedDependencies": {
      "@auth/express@0.5.5": "patches/@auth__express@0.5.5.patch",
      "@auth/core@0.31.0": "patches/@auth__core@0.31.0.patch",
      "@auth/core@0.32.0": "patches/@auth__core@0.32.0.patch",
      "@auth/prisma-adapter@2.2.0": "patches/@auth__prisma-adapter@2.2.0.patch"
    }
  }
}
