{
  "eslint.workingDirectories": [
    {
      "pattern": "./apps/*/"
    },
    {
      "pattern": "./packages/*/"
    }
  ],
  "tailwindCSS.experimental.classRegex": [
    [
      "cva\\(([^)]*)\\)",
      "[\"'`]([^\"'`]*).*?[\"'`]",
      "cn\\(.*\\)",
      "clsx\\(.*\\)"
    ]
  ],
  "tailwindCSS.classAttributes": [".*ClassName*"],
  "eslint.format.enable": true,
  "files.associations": {
    "*.css": "tailwindcss"
  }
}
